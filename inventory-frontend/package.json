{
  "name": "inventory-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start:cors": "ng serve --disable-host-check --public-host http://localhost:4200",
    "start:secure": "ng serve --ssl true --ssl-key ssl/server.key --ssl-cert ssl/server.crt",
    "test": "ng test",
    "serve:ssr:inventory-frontend": "node dist/inventory-frontend/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.4",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "bootstrap": "^5.3.8",
    "express": "^5.1.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.26.17",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
